		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5"
	},
	"devDependencies": {
		"@types/bun": "^1.1.2",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"eslint": "^8.57.0",
